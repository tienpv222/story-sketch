  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "paths": {
      "/src/*": ["./src/*"]
    },
    "types": ["vite/client"]
  }
}
